{
  "name": "lyrics-video-maker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@remotion/bundler": "^4.0.278",
    "@remotion/google-fonts": "^4.0.278",
    "@remotion/player": "^4.0.278",
    "@remotion/renderer": "^4.0.278",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-tabs": "^5.0.4",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^7.4.0",
    "react-scripts": "5.1.0-next.26",
    "react-tabs": "^6.1.0",
    "remotion": "^4.0.278",
    "styled-components": "^6.1.15",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@remotion/cli": "^4.0.278",
    "@remotion/eslint-plugin": "^4.0.278",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "start": "cross-env PORT=3002 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "studio": "remotion studio src/remotion/index.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "server:start": "cross-env PORT=3003 node server/dist/index.js",
    "server:dev": "concurrently \"npm run start\" \"npm run server:start\"",
    "postinstall": "npm run server:build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "@remotion"
    ],
    "overrides": [
      {
        "files": [
          "src/remotion/*.{ts,tsx}"
        ],
        "extends": [
          "plugin:@remotion/recommended"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
